# Change the name of this file to .env after updating it!

POSTGRES_USER=root
POSTGRES_PASSWORD=password
POSTGRES_DB=n8n

N8N_ENCRYPTION_KEY=super-secret-key
N8N_USER_MANAGEMENT_JWT_SECRET=even-more-secret

HYTOPIA_PORT=8080
HYTOPIA_EXAMPLES_PATH=./examples
CUSTOM_GAME_PATH=./game-repos/custom-game
ENABLE_OFFICIAL_EXAMPLES=true
ENABLE_CUSTOM_GAME=true

# Examples configuration
EXAMPLES: |
  - name: entity-controller
    path: examples/entity-controller
  - name: entity-spawn
    path: examples/entity-spawn

# Repository configuration
# For local repos:
# - name: example-game
#   path: example-game
# For git repos, just the url (name and path will be auto-created from repo name):
GAME_REPOS=|
  - url: https://github.com/username/repo-name.git

# Cloudflare configuration (optional - remove or leave empty for local-only development)
# CLOUDFLARE_TUNNEL_TOKEN=your-tunnel-token
# TUNNEL_ID=your-tunnel-id
# TUNNEL_DOMAIN=your-domain.com 